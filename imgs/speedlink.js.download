function timeit(){var e=new Date;var t=e.getMinutes();if(t<10){t="0"+t}$(".timelist i").each(function(){var e=$(this).html();var r=e.split(":");if(r[1]=="59"&&t=="00"){if(r[0]==23){r[0]="00"}else{r[0]++;if(r[0]<10){r[0]="0"+r[0]}}}r[1]=t;var i=r.join(":");$(this).html(i)})}  


// sameday
$(document).ready(function() {
  //select vechicle
  $('.quote-types li').click(function(event) {
    
    $( "input[name=vehicle]:radio").removeAttr("checked");
    $('.quote-types li').removeClass( "current" );
    $(this).addClass("current");
    $(this).find("[type=radio]").addClass("yay").attr("checked", "checked");
    
  });
	// process the quote
	$('#sdq').submit(function(event) {
  	
  	if(isValidPostcode($('input[name=pcf]').val()) && isValidPostcode($('input[name=pct]').val())) {
  	  
  	  //validation
      $('#sdq').fadeOut();
      $('#results').html("<h1>Getting your quote...<h1>").show();
      $('#waiting').fadeIn();
  		// get the form data
  		if ($('#waitandreturn').is(':checked')) {
    		var waitandreturn = true;
    		//console.log('true');
  		} else {
   			var waitandreturn = false;
    		//console.log('false');
  		}
		
		  //into json		
  		var formData = {
  			'service' 				: 'UK to UK Same Day',
  			'fromPostCode' 		: $('input[name=pcf]').val(),
  			'toPostCode' 		  : $('input[name=pct]').val(),
  			'vehicleType' 	  : $('input[name=vehicle]:checked').val(),
  			'waitAndReturn'   : waitandreturn
  		};
  		// process the form  		
  		$.ajax({
  			type 		 : 'POST',           // define the type of HTTP verb we want to use (POST for our form)
  			url 		 : '/quotenow.php',  // the url where we want to POST (local to get around CORS)
  			data 		 : formData,         // our data object
  			dataType : 'json',           // what type of data do we expect back from the server
        encode   : true
  		}).done(function(data) {
          $('#results').hide();
          if(data.error === null) {
            $('#results').html("<h1>Your online price</h1><h2>£" +data.price+"</h2><p class='ac'><a href='http:\/\/www.slt-intership.com/QuoteEngine/GetQuote.aspx' class='btn btn-primary'>Book online</a></p>").fadeIn();
          } else {
            $('#results').html("<h1>Sorry</h1><p>An error occured and we were unable to get you an online quote right now, please call us</p><p>The error was <strong>"+data.error+"<strong> <br><a onclick='resetform();' href='#'>retry</a></p>").fadeIn();
          }
  		});
		
		} else {
  		
  		$('#sdq').prepend('<p>Please enter a valid to and from postcode</p>');
  		
		}
		// stop the form from submitting the normal way and refreshing the page
		event.preventDefault();
	});
	
	$.ajax({
  dataType: "jsonp",
    url: 'https://api.ipinfodb.com/v3/ip-country/?key=b4bf08ea5ea0aea2ae11310c8f3e0b70a4736fc19d3e07176c510d3d7ac41052&format=json' ,
    }).done(function ( data ) {
      if(data.countryCode=="CA" && document.cookie.indexOf("notran") === -1) {
        if (document.location.href.indexOf('/ca/') === -1){ 
            showCountryRedirect();              
        }
      }
  });
  
  if(document.cookie.indexOf("alertme") === -1) {
    $('#abnneralert').show();
  }
  	
});

function showCountryRedirect() {
  $("body").prepend('<div id="canadadirective" style="position: fixed; top: 0px; left: 0px; width: 100%; height: auto; color: rgb(255, 255, 255); font-size: 13px; text-align: center; z-index: 999999; background: #18397d;"><div style="position:relative;height:auto;width:90%;padding:10px;margin-left:auto;margin-right:auto;">Hi, you appear to be from Canada - would you like to view our Canada services? <br><span style="font-size:20px;"><a href="/ca/toronto/" style="color:#fff" onClick="canadayes();" >Toronto</a> or <a href="/ca/calgary/" onClick="canadayes();"  style="color:#fff">Calgary</a></span> <br><div style="margin-top:5px;"><input type="submit" name="impliedsubmit" id="impliedsubmit" value="No thanks" class="btn btn-primary" onClick="noCanada();"></div></div></div>');
}

function noCanada() {
  $("#canadadirective").hide();
  document.cookie = "notran=true; max-age=" + 60 * 60 * 24 * 10; // 60 seconds to a minute, 60 minutes to an hour, 24 hours to a day, and 10 days.
}

function canadayes() {
  document.cookie = "canada=true; max-age=" + 60 * 60 * 24 * 10; // 60 seconds to a minute, 60 minutes to an hour, 24 hours to a day, and 10 days.  
}

function isValidPostcode(p) { 
    var postcodeRegEx = /[A-Z]{1,2}[0-9]{1,2} ?[0-9][A-Z]{2}/i; 
    return postcodeRegEx.test(p); 
}

function resetform() {
  $('#results').hide();
  $('#sdq').show();
}

function hideAlertBanner() {
  $('#abnneralert').hide();
  document.cookie = "alertme=false; max-age=" + 60 * 60 * 2; // 60 seconds to a minute, 60 minutes to an hour
}

